datasource db {
  provider = "postgres"
  url      = "postgres://jm:jm@localhost:5432/ecole"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  name   String
  email  String @unique
  password String
}

model Inscription {
  id    Int    @id @default(autoincrement())
  name   String
  genre  String
  phone String
  section String
  option String
  classesollicite String
  ecoleprovenance String
  classeprovenance String
  pourcentage String
}